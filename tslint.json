{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "max-line-length": [
      true,
      150
    ],
    "indent": [
      true,
      "spaces"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [
      true,
      "single"
    ],
    "no-console": false,
    "no-unused-expression": false,
    "variable-name": {
      "options": [
        "allow-leading-underscore"
      ]
    }
  },
  "rulesDirectory": []
}
